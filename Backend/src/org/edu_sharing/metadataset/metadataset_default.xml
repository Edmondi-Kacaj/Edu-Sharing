						<labelkey>ccm_prop_remoteobject_repid</labelkey>
						<validators>
							<validator type="MANDATORY" />
						</validators>
					</property>
					<property name="{http://www.campuscontent.de/model/1.0}remotenodeid">
						<type>{http://www.campuscontent.de/model/1.0}remoteobject</type>
						<labelkey>ccm_prop_remoteobject_nodeid</labelkey>
						<validators>
							<validator type="MANDATORY" />
						</validators>
					</property>
				</properties>
			</panel>
		</form>
