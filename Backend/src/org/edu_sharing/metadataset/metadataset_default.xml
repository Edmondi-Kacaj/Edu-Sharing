		<form id="{http://www.campuscontent.de/model/1.0}map">
			<panel name="metadataset">
				<oncreate>false</oncreate>
				<onupdate>false</onupdate>
				<labelkey>cm_aspect_metadataset</labelkey>
				<properties>
					<property name="{http://www.alfresco.org/model/content/1.0}edu_metadataset">
						<type>{http://www.campuscontent.de/model/1.0}map</type>
						<labelkey>cm_aspect_metadataset</labelkey>
						<widget>listbox</widget>
						<!-- valuespace>/org/edu_sharing/metadataset/valuespaces.xml</valuespace> <valuespace_i18n>org.edu_sharing.metadataset.valuespaces_i18n</valuespace_i18n -->
						<formlength>206px</formlength>
						<!-- formheight>100</formheight -->
					</property>
					<property name="{http://www.alfresco.org/model/content/1.0}edu_forcemetadataset">
						<type>{http://www.campuscontent.de/model/1.0}map</type>
						<labelkey>cm_aspect_metadataset_forcemetadataset</labelkey>
					</property>
				</properties>
			</panel>
			<panel name="map" styleName="mapData">
				<labelkey>ccm_type_map</labelkey>
				<properties>
					<property name="{http://www.alfresco.org/model/content/1.0}name">
						<type>{http://www.campuscontent.de/model/1.0}map</type>
						<labelkey>cm_prop_title</labelkey>
						<validators>
							<validator type="MANDATORY" />
						</validators>
					</property>
					<property name="{http://www.campuscontent.de/model/1.0}mapicon">
						<type>{http://www.campuscontent.de/model/1.0}map</type>
						<labelkey>ccm_prop_map_icon</labelkey>
					</property>
					<property name="{http://www.campuscontent.de/model/1.0}metadataPresetting">
						<type>{http://www.campuscontent.de/model/1.0}map</type>
						<labelkey>metadatapresetting</labelkey>
						<widget>metadatapresetting</widget>
						<params>
							<param>
								<key>metadata_presetting_properties</key>
								<value>{http://www.campuscontent.de/model/lom/1.0}general_keyword</value>
							</param>
						</params>
					</property>
				</properties>
			</panel>
		</form>
		<form id="{http://www.campuscontent.de/model/1.0}io">
			<panel name="Basic" styleName="basicPanel" multiupload="true">
				<labelkey>dialog_upload_tab_basic</labelkey>
				<oncreate>false</oncreate>
				<properties>
					<property name="{http://www.alfresco.org/model/content/1.0}name">
						<type>{http://www.campuscontent.de/model/1.0}io</type>
						<labelkey>lom_prop_general_title</labelkey>
						<labelhint>lom_prop_general_title_labelhint</labelhint>
						<!-- <widget>textfield</widget>-->
						<widget>name</widget>
						<copyfrom>{http://www.campuscontent.de/model/1.0}filename,{http://www.campuscontent.de/model/1.0}wwwurl</copyfrom>
						<validators>
							<validator type="MANDATORYTITLE" />
						</validators>
						<formlength>380px</formlength>
					</property>
					<property name="{http://www.campuscontent.de/model/lom/1.0}general_description">
						<type>{http://www.campuscontent.de/model/1.0}io</type>
						<labelkey>lom_prop_general_description</labelkey>
						<widget>textarea</widget>
						<formlength>300px</formlength>
						<formheight>100px</formheight>
					</property>
					<property name="{http://www.campuscontent.de/model/lom/1.0}general_keyword">
						<type>{http://www.campuscontent.de/model/1.0}io</type>
						<labelkey>lom_prop_general_keyword</labelkey>
						<!-- <widget>multivaluetextboxform</widget>-->
						<widget>multivaluedynamicsuggestbox</widget>
						<formlength>300px</formlength>
						<params>
							<param>
								<key>search_suggestbox_properties</key>
								<value>{http://www.campuscontent.de/model/lom/1.0}general_keyword.__u,{http://www.campuscontent.de/model/lom/1.0}general_keyword.u</value>
							</param>
						</params>
					</property>
					<property name="{http://www.campuscontent.de/model/1.0}userdefined_preview">
						<type>{http://www.campuscontent.de/model/1.0}io</type>
						<widget>preview</widget>
					</property>
					<property name="{http://www.alfresco.org/model/content/1.0}content">
						<type>{http://www.campuscontent.de/model/1.0}io</type>
						<widget>file_link</widget>
					</property>
					<property name="{http://www.campuscontent.de/model/1.0}version_comment">
