<model name="ccm:campuscontentmodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<description>Campus Content custom Model</description>
	<author>Daniel Rudolph</author>
	<version>1.0</version>
	<imports>
		<!-- Import Alfresco Dictionary Definitions -->
		<import uri="http://www.alfresco.org/model/dictionary/1.0"
			prefix="d" />
		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<import uri="http://www.campuscontent.de/model/lom/1.0" prefix="cclom" />
		<import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
	</imports>
	<namespaces>
		<!-- Define a Namespace for my new definitions -->
		<namespace uri="http://www.campuscontent.de/model/1.0"
			prefix="ccm" />
	</namespaces>

	<!-- Type and Aspect definitions go here -->
	<types>
		<type name="ccm:repository">
			<title>Repository</title>
			<parent>cm:folder</parent>
		</type>
		<type name="ccm:saved_search">
			<title>Campus Content stored search queries</title>
			<parent>cm:cmobject</parent>
			<properties>
				<property name="ccm:saved_search_repository">
						<type>d:text</type>
				</property>
				<property name="ccm:saved_search_mds">
						<type>d:text</type>
				</property>
				<property name="ccm:saved_search_query">
						<type>d:text</type>
				</property>
				<property name="ccm:saved_search_parameters">
						<type>d:text</type>
				</property>
			</properties>
			<mandatory-aspects>
    			<aspect>ccm:eduscope</aspect>
   			</mandatory-aspects>
		</type>
		<type name="ccm:remoteobject">
			<title>Campus Content foreign repository object</title>
			<parent>cm:content</parent>
			<properties>
				<property name="ccm:remoterepositoryid">
					<type>d:text</type>
				</property>
				<property name="ccm:remotenodeid">
					<type>d:text</type>
				</property>
				<property name="ccm:remoterepositorytype">
					<type>d:text</type>
				</property>
			</properties>
			<mandatory-aspects>
				<aspect>ccm:usageaspect</aspect>
			</mandatory-aspects>
		</type>


		<type name="ccm:sysupdate">
			<title>edu-sharing system update marker</title>
			<parent>cm:content</parent>
			<properties>
				<property name="ccm:sysupdate_id">
					<type>d:text</type>
				</property>
				<property name="ccm:sysupdate_date">
					<type>d:date</type>
				</property>
			</properties>
		</type>


		<type name="ccm:io">
			<title>Campus Content IO</title>
			<parent>cm:content</parent>
			<includedInSuperTypeQuery>true</includedInSuperTypeQuery>
			<properties>
				<!--0 = IO (Informationsobject), 1 = LS (Lernszenario) -->
				<property name="ccm:objecttype">
					<type>d:int</type>
				</property>
			</properties>
			<associations>
				<association name="ccm:ccrelatedio">
					<target>
						<class>ccm:io</class>
						<mandatory>false</mandatory>
						<many>false</many>
					</target>
				</association>
				<child-association name="ccm:childio">
					<source>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>ccm:io</class>
						<mandatory>false</mandatory>
						<many>true</many>
					</target>
				</child-association>
			</associations>
			<mandatory-aspects>
				<aspect>ccm:iometadata</aspect>
				<aspect>cclom:schema</aspect>
				<aspect>ccm:usageaspect</aspect>
				<aspect>cm:thumbnailed</aspect>
				<aspect>ccm:lomreplication</aspect>
				<aspect>ccm:licenses</aspect>
				<aspect>ccm:commonlicenses</aspect>
				<aspect>cm:metadataset</aspect>
				<aspect>ccm:eduscope</aspect>
			</mandatory-aspects>
		</type>
		
		<type name="ccm:notify">
			<title>Edu-Sharing notify object</title>
			<parent>sys:base</parent>
			<properties>
				<property name="ccm:notify_event">
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
				<property name="ccm:notify_action">
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ccm:notify_user">
					<type>d:text</type>
					<mandatory>true</mandatory>
				</property>
				<!-- when notify_event is "PERMISSION" notify_context is the AuthorityName which was invited/uninvited-->
				<property name="ccm:notify_context">
					<type>d:text</type>
					<mandatory>false</mandatory>			
				</property>
			</properties>
			<associations>
				<child-association name="ccm:notify_nodes">
					<source>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>cm:cmobject</class>
						<mandatory>false</mandatory>
						<many>true</many>
					</target>
				</child-association>
			</associations>
			<!-- for create date -->
			<mandatory-aspects>
				<aspect>cm:auditable</aspect>
			</mandatory-aspects>
		</type>


		<!-- personal licenes that can be assigned to persons or groups -->
		<type name="ccm:assignedlicense">
			<title>Campus Content License,Authority</title>
			<parent>cm:cmobject</parent>
			<properties>
				<property name="ccm:authority">
					<type>d:text</type>
				</property>
				<property name="ccm:assignedlicense">
					<type>d:text</type>
					<multiple>true</multiple>
				</property>
				<property name="ccm:assignedlicenseExpiry">
					<type>d:datetime</type>
					<index enabled="true">
                  		<atomic>true</atomic>
                  		<stored>false</stored> 
                  		<tokenised>both</tokenised>
                  		<facetable>true</facetable>
               		</index>
				</property>
			</properties>
		</type>

		
		<!-- direktfreigabe von cm:cmobject anstatt sys:base damit modiefied, created usw. gesetzt werden -->
		<type name="ccm:share">
			<title>direct share</title>
			<parent>cm:cmobject</parent>
			<properties>
				<property name="ccm:share_expirydate">
					<type>d:long</type>
				</property>
				<property name="ccm:share_mail">
					<type>d:text</type>
				</property>
				<property name="ccm:share_token">
					<type>d:text</type>
				</property>
				<property name="ccm:share_download_counter">
					<type>d:int</type>
				</property>
			</properties>
		</type>

		<type name="ccm:map">
			<title>Campus Content Map</title>
			<parent>cm:folder</parent>
			<includedInSuperTypeQuery>true</includedInSuperTypeQuery>
			<properties>
			</properties>
			<mandatory-aspects>
				<aspect>ccm:mapdata</aspect>
				<aspect>ccm:licenses</aspect>
				<aspect>cm:metadataset</aspect>
				<aspect>ccm:eduscope</aspect>
			</mandatory-aspects>
		</type>



		<type name="ccm:maprelation">
			<title>Campus Content Map Relation</title>
			<parent>cm:cmobject</parent>
			<properties>
			</properties>
			<associations>
				<association name="ccm:reltarget">
					<source>
						<role>ccm:isRelation</role>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>cm:cmobject</class>
						<role>ccm:hasTargetRelation</role>
						<mandatory>false</mandatory>
						<many>false</many>
					</target>
				</association>
				<association name="ccm:relsource">
					<source>
						<role>ccm:isRelation</role>
						<mandatory>false</mandatory>
						<many>true</many>
					</source>
					<target>
						<class>cm:cmobject</class>
						<role>ccm:hasSourceRelation</role>
						<mandatory>false</mandatory>
						<many>false</many>
					</target>
				</association>
				<association name="ccm:reldirection">
					<target>
						<class>cm:cmobject</class>
						<mandatory>false</mandatory>
						<many>false</many>
					</target>
				</association>
			</associations>
		</type>

		<type name="ccm:usage">
			<title>Campus Content Object Usage Entity</title>
			<parent>cm:cmobject</parent>
			<archive>false</archive>
			<properties>
				<property name="ccm:usageappid">
					<type>d:text</type>
				</property>
				<property name="ccm:usagecourseid">
					<type>d:text</type>
				</property>
				<property name="ccm:usageparentnodeid">
					<type>d:text</type>
				</property>
				<!-- user from the application (i.e. LMS) -->
				<property name="ccm:usageappuser">
					<type>d:text</type>
				</property>
				<!-- globel uid (i.e. from LDAP) -->
				<property name="ccm:usageguid">
					<type>d:text</type>
				</property>
				<property name="ccm:usageappusermail">
					<type>d:text</type>
				</property>
				<property name="ccm:usagefrom">
					<type>d:date</type>
				</property>
				<property name="ccm:usageto">
					<type>d:date</type>
				</property>
				<property name="ccm:usagemaxpersons">
					<type>d:int</type>
				</property>
				<property name="ccm:usagecounter">
					<type>d:int</type>
				</property>
				<property name="ccm:usageversion">
					<type>d:text</type>
				</property>
				<property name="ccm:usageressourceid">
					<type>d:text</type>
				</property>
				<property name="ccm:usagexmlparams">
					<type>d:text</type>
				</property>
			</properties>
		</type>

		<!-- organisation, persons can be part of -->
		<type name="ccm:organisation">
			<title>Organisation</title>
			<parent>cm:cmobject</parent>
			<properties>
				<property name="ccm:org_name">
					<type>d:mltext</type>
				</property>
				<property name="ccm:org_description">
					<type>d:mltext</type>
				</property>
				<property name="ccm:org_url">
					<type>d:text</type>
				</property>
				<property name="ccm:org_mail">
					<type>d:text</type>
				</property>
				<property name="ccm:org_default">
					<type>d:boolean</type>
				</property>
				<property name="ccm:org_contact_name">
					<type>d:text</type>
				</property>
				<property name="ccm:org_contact_firstname">
					<type>d:text</type>
				</property>
				<property name="ccm:org_contact_mail">
					<type>d:text</type>
				</property>
			</properties>
			<mandatory-aspects>
				<aspect>cm:metadataset</aspect>
			</mandatory-aspects>
		</type>
		
		<type name="ccm:toolpermission">
       		<title>Tool Permission</title>
         	<parent>ccm:io</parent>
       </type>
       
       <type name="ccm:tool_instance">
       		<title>Tool Instance</title>
       		<parent>cm:cmobject</parent>
         	<properties>
         		<property name="ccm:tool_instance_key">
         			<type>d:text</type>	
         		</property>
         		<property name="ccm:tool_instance_secret">
         			<type>d:text</type>	
         		</property>
         		<property name="ccm:tool_instance_params">
         			<type>d:text</type>	
         		</property>
         		<property name="ccm:tool_instance_license">
         			<type>d:text</type>	
         		</property>
         		<property name="ccm:tool_instance_support_contact">
         			<type>d:text</type>	
         		</property>
         		<property name="ccm:tool_instance_support_mail">
         			<type>d:text</type>	
         		</property>
         		<property name="ccm:tool_instance_provider">
         			<type>d:text</type>	
         		</property>
         		<property name="ccm:tool_instance_provider_url">
         			<type>d:text</type>	
         		</property>
         		<property name="ccm:tool_instance_access">
         			<type>d:text</type>	
         		</property>
         	</properties>
       </type>
	</types>


	<aspects>

		<aspect name="ccm:ressourceinfo">
			<title>Aspect for additional resource Information like scorm, qti
				Version</title>
			<properties>
				<property name="ccm:ccressourcetype">
					<type>d:text</type>
				</property>
				<property name="ccm:ccresourcesubtype">
					<type>d:text</type>
					<multiple>true</multiple>
				</property>
				<property name="ccm:ccressourceversion">
					<type>d:text</type>
				</property>
				<property name="ccm:ccresourcecontent">
					<type>d:content</type>
				</property>
			</properties>
		</aspect>
		
		<aspect name="ccm:userExtension">
			<title>Aspect for additional user information like profile, node lists etc.
				Version</title>
			<properties>
				<property name="ccm:userPreferences">
               <type>d:text</type>
               <mandatory>true</mandatory>
	            </property>
	            <property name="ccm:userNodeLists">
	               <type>d:text</type>
	               <mandatory>true</mandatory>
	            </property>
			</properties>
		</aspect>

		<aspect name="ccm:usageaspect">
			<title>Aspect for Usage Tracking</title>
			<properties></properties>
			<associations>
				<child-association name="ccm:usages">
					<source>
						<mandatory>false</mandatory>
						<!-- because of checkout Problem set many to true -->
						<many>true</many>
					</source>
					<target>
						<class>ccm:usage</class>
						<mandatory>false</mandatory>
						<many>true</many>
					</target>
				</child-association>
			</associations>
		</aspect>

		<aspect name="ccm:mapdata">
			<title>Data for CC mapable objects</title>
			<properties>
				<property name="ccm:mapicon">
					<type>d:content</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ccm:maptype">
					<type>d:text</type>
				</property>
			</properties>
		</aspect>

		<aspect name="ccm:basemetadata">
			<title>Metadata for a CC base object</title>
			<properties>
				<property name="ccm:publisher">
					<type>d:text</type>
				</property>

				<property name="ccm:topic">
					<type>d:text</type>
				</property>
				<property name="ccm:license">
					<type>d:text</type>
					<multiple>true</multiple>
				</property>
				<property name="ccm:linktype">
					<type>d:text</type>
				</property>
				<property name="ccm:wwwurl">
					<type>d:text</type>
					<index enabled="true">
						<tokenised>false</tokenised>
					</index>
				</property>
				<property name="ccm:filename">
					<type>d:text</type>
				</property>
			</properties>
		</aspect>

		<aspect name="ccm:iometadata">
			<title>Metadata for an information object</title>
			<parent>ccm:basemetadata</parent>
			<properties>
				<property name="ccm:mediatype">
					<type>d:text</type>
				</property>
				<property name="ccm:semantictype">
					<type>d:text</type>
				</property>
				
				<!--  this is for collection ref objects to remember the original sys:node-uuid
					 for the original object itself the ccm:original = sys:node-uuid
					 
					 it's used to collapse multiple refobjects and the original to one when using the search
				 -->
				<property name="ccm:original">
					<type>d:text</type>
					<index enabled="true">
	                  <atomic>true</atomic>
	                  <stored>false</stored>
	                  <tokenised>false</tokenised>
	               </index>
				</property>
				<property name="ccm:groupsize">
					<type>d:int</type>
				</property>
				<property name="ccm:learninggoal">
					<type>d:mltext</type>
				</property>
				<!--Handlungsempfehlung Lehrende -->
				<property name="ccm:guidanceteachers">
					<type>d:mltext</type>
				</property>
				<property name="ccm:guidancestudents">
					<type>d:mltext</type>
				</property>
				<!--Bewertung -->
				<property name="ccm:pointsfrom">
					<type>d:int</type>
				</property>
				<property name="ccm:pointsto">
					<type>d:int</type>
				</property>
				<property name="ccm:pointsdefault">
					<type>d:int</type>
				</property>
				<!-- remember the source and the id of the replicated object -->
				<property name="ccm:replicationsource">
					<type>d:text</type>
				</property>
				<property name="ccm:replicationsourceid">
					<type>d:text</type>
				</property>
				<property name="ccm:replicationsourcetimestamp">
					<type>d:text</type>
				</property>
				<!-- for elixier objects thumbnail url -->
				<property name="ccm:thumbnailurl">
					<type>d:text</type>
				</property>
				<!-- for elixier serientitel -->
				<property name="ccm:title_series">
					<type>d:text</type>
				</property>

				<!-- userdefined preview image -->
				<property name="ccm:userdefined_preview">
					<type>d:content</type>

					<!--d:content can not be mutivalued <multiple>true</multiple> -->
				</property>
				
				<!-- 
					save if user likes to have a new version
				 	this will be set to false in create servlet and to true in update if not versionhistory exsists
					webdav/cifs/synch will create a version by default
				-->
				<property name="ccm:create_version">
					<type>d:boolean</type>
               		<default>true</default>			
				</property>
				<property name="ccm:version_comment">
					<type>d:text</type>
				</property>
				
				<property name="ccm:schoolcontext">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	                  <atomic>true</atomic>
	                  <stored>false</stored>
	                  <tokenised>false</tokenised>
	               </index>
				</property>
				<property name="ccm:schooltopic">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	                  <atomic>true</atomic>
	                  <stored>false</stored>
	                  <tokenised>false</tokenised>
	               </index>
				</property>
				<property name="ccm:courseofstudies">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	                  <atomic>true</atomic>
	                  <stored>false</stored>
	                  <tokenised>false</tokenised>
	               </index>
				</property>
				<property name="ccm:university">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	                  <atomic>true</atomic>
	                  <stored>false</stored>
	                  <tokenised>both</tokenised>
	               </index>
				</property>
				<!-- this for customers that got an keyword catalog that comes with keyword id's.
					we use this prop to allow full text search 
					and building a tagcloud without the need to get caption by asking the async bigdata valuespace service -->
				<property name="ccm:generalkeyword_caption">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	                  <atomic>true</atomic>
	                  <stored>false</stored>
	                  <tokenised>both</tokenised>
	               </index>
				</property>
				
				<!-- Klassenstufe -->
				<property name="ccm:classlevel">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	         			<atomic>true</atomic>
	          			<stored>false</stored>
	         			<tokenised>both</tokenised>
	         		</index>										
				</property>		
				<!-- Schulart -->
				<property name="ccm:typeofschool">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	         			<atomic>true</atomic>
	          			<stored>false</stored>
	         			<tokenised>both</tokenised>
	         		</index>
				</property>
				<property name="ccm:competence">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	         			<atomic>true</atomic>
	          			<stored>false</stored>
	         			<tokenised>both</tokenised>
	         		</index>										
				</property>	
				<!-- Lehrplan -->
				<property name="ccm:curriculum">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	         			<atomic>true</atomic>
	          			<stored>false</stored>
	         			<tokenised>both</tokenised>
	         		</index>										
				</property>
				
				<property name="ccm:mediacenter">
					<type>d:text</type>
					<multiple>true</multiple>
				</property>
				
				<property name="ccm:editorType">
					<type>d:text</type>
					<index enabled="true">
	                  <atomic>true</atomic>
	                  <stored>false</stored>
	                  <tokenised>false</tokenised>
	               </index>
				</property>
				
			</properties>
		</aspect>

		<!-- for faster access to LOM Metadata -->
		<aspect name="ccm:lomreplication">
			<title>redundant LOM Metadata for faster access when searching
			</title>
			<properties>
				<property name="ccm:educationallearningresourcetype">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	                  <atomic>true</atomic>
	                  <stored>false</stored>
	                  <tokenised>both</tokenised>
	               </index>
				</property>
				<property name="ccm:educationalcontext">
					<type>d:text</type>
					<multiple>true</multiple>
					<index enabled="true">
	         			<atomic>true</atomic>
	          			<stored>false</stored>
	         			<tokenised>both</tokenised>
	         		</index>
				</property>
				<property name="ccm:educationaldifficulty">
					<type>d:text</type>
				</property>
				<property name="ccm:educationalinteractivitytype">
					<type>d:text</type>
					<multiple>true</multiple>
				</property>
				<property name="ccm:educationaltypicalagerange">
					<type>d:text</type>
					<multiple>true</multiple>
				</property>
