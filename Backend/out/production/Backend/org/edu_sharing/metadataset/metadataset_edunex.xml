	<queries>
		<query join="AND" labelkey="search2_pw_search" criteriaboxid="search2_pw_search">
			<widget>org.edu_sharing.repository.client.search.MetadataSetQueryGuiMaster</widget>
			<property name="searchword">
				<widget>multivaluetextbox</widget>
				<statement><![CDATA[<condition operator="AND" field='titel_fields'>${value}</condition>]]></statement>
				<labelkey>cap_searchword</labelkey>
				<multiple>true</multiple>
				<multiplejoin>AND</multiplejoin>
				<stylename>searchword</stylename>
				<validators>
					<validator type="MINCHARS"/>
				</validators>
				<init_by_get_param>p_searchtext</init_by_get_param>
			</property>
			<!--uncomment the following property if there are federated repositories in your ccapp-registry.properties.xml, 
			    the valuespace will be filled by the application
			-->
			
			<property name="EDU_SHARING_REPOSITORIES">
				<widget>multivaluesuggestbox</widget>
				<toggle>true</toggle>
				<labelkey>repositiories</labelkey>	
			</property>
			
			<property name="{http://www.campuscontent.de/model/1.0}educationalcontext">
				<widget>listbox</widget>
				<statement>@ccm\:educationalcontext:"*${value}*"</statement>
				<valuespace_key>{http://www.campuscontent.de/model/lom/1.0}context</valuespace_key>
				<valuespace>/org/edu_sharing/metadataset/valuespaces.xml</valuespace>
				<valuespace_i18n_prefix>cb_educational_context_</valuespace_i18n_prefix>
				<toggle>true</toggle>
				<labelkey>lom_prop_educational_context</labelkey>
				<stylename>context</stylename>
			</property>
			<property name="{http://www.campuscontent.de/model/1.0}taxonid">
				<widget>multivaluesuggestbox</widget>
				<statement>@ccm\:taxonid:"${value}*"</statement>
				<valuespace_i18n_prefix>ddc_L1_</valuespace_i18n_prefix>
				<valuespace>/org/edu_sharing/metadataset/valuespace_ddc_level1.xml</valuespace>
				<valuespace_i18n>org.edu_sharing.metadataset.valuespaces_i18n</valuespace_i18n>
				<multiple>true</multiple>
				<multiplejoin>AND</multiplejoin>
				<toggle>true</toggle>
				<labelkey>input_taxonentry</labelkey>
				<stylename>taxon_entry</stylename>
			</property>
			<property name="{http://www.campuscontent.de/model/lom/1.0}learningresourcetype">
				<widget>listbox</widget>
				<statement>@ccm\:educationallearningresourcetype:"*${value}*"</statement>
				<valuespace>/org/edu_sharing/metadataset/valuespaces.xml</valuespace>
				<valuespace_i18n>org.edu_sharing.metadataset.valuespaces_i18n</valuespace_i18n>
				<formheight>50px</formheight>
				<multiple>true</multiple>
				<multiplejoin>AND</multiplejoin>
				<toggle>true</toggle>
				<labelkey>lom_prop_educational_learningresourcetype</labelkey>
			</property>
		</query>
		<query labelkey="" criteriaboxid="search2_pw_fields_of_expertise">
			<property name="{http://www.campuscontent.de/model/1.0}taxonid">
				<widget>searchcategories</widget>
				<statement>(@ccm\:taxonid:"${value}*")</statement>
				<valuespace_i18n_prefix>ddc_L1_</valuespace_i18n_prefix>
				<valuespace>/org/edu_sharing/metadataset/valuespace_ddc_level1.xml</valuespace>
				<valuespace_i18n>org.edu_sharing.metadataset.valuespaces_i18n</valuespace_i18n>
				<labelkey>search2_prop_taxon</labelkey>
				<stylename>cc_search2_categories</stylename>
				<params>
					<param>
						<key>countprops_substring</key>
						<value>1</value>
					</param>
				</params>
			</property>
		</query>
		<query labelkey="" criteriaboxid="search2_pw_context">
			<property name="{http://www.campuscontent.de/model/1.0}educationalcontext">
				<widget>searchcategories</widget>
				<statement>@ccm\:educationalcontext:"${value}"</statement>
				<valuespace_key>{http://www.campuscontent.de/model/lom/1.0}context</valuespace_key>
				<valuespace>/org/edu_sharing/metadataset/valuespaces.xml</valuespace>
				<valuespace_i18n>org.edu_sharing.metadataset.valuespaces_i18n</valuespace_i18n>
				<labelkey>lom_prop_educational_context</labelkey>
				<stylename>cc_search2_categories</stylename>
				<!--  wegen bindestrich nicht escapen -->
				<escape>false</escape>
			</property>
		</query>
		<query labelkey="" criteriaboxid="tagcloud">
			<property name="{http://www.campuscontent.de/model/lom/1.0}general_keyword">
				<widget>tagcloud</widget>
				<statement>@cclom\:general_keyword:"${value}"</statement>
				<multiple>true</multiple>
				<multiplejoin>AND</multiplejoin>
				<labelkey>search2_prop_general_keyword</labelkey>
				<escape>false</escape>
				<stylename>cc_search2_tagcloud_container</stylename>
			</property>
			<property name="{http://www.campuscontent.de/model/1.0}educationallearningresourcetype">
				<widget>tagcloud</widget>
				<statement>@ccm\:educationallearningresourcetype:"${value}"</statement>
				<multiple>true</multiple>
				<multiplejoin>AND</multiplejoin>
				<labelkey>lom_prop_educational_learningresourcetype</labelkey>
				<escape>false</escape>
				<stylename>cc_search2_tagcloud_container</stylename>
			</property>
		</query>
